{
    "name": "drewmw5/breeze",
    "description": "Minimal Laravel authentication scaffolding with React in Typescript",
    "keywords": ["laravel", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/drewmw5/breeze/issues",
        "source": "https://github.com/drewmw5/breeze"
    },
    "authors": [
        {
            "name": "Drew Williams",
            "email": "drewmwilliams55@gmail.com"
        }
    ],
    "require": {
        "laravel/breeze": "^1.18.0",
        "php": "^8.0.2",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/filesystem": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0",
        "illuminate/validation": "^9.21|^10.0"
    },
    "conflict": {
        "laravel/framework": "<9.37.0"
    },
    "autoload": {
        "psr-4": {
            "Drewmw5\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "merge-dev"
        },
        "laravel": {
            "providers": [
                "Drewmw5\\Breeze\\ReactTSServiceProvider"
            ],
            "dont-discover":[
                "laravel/breeze"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
